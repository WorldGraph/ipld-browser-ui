  "scripts": {
    "checkNpmUpdates": "ncu --reject \"webpack,@emotion/core,@emotion/css,emotion,copy-webpack-plugin,webpack-cli,html-webpack-plugin,ts-loader,style-loader,ts-node\"",
    "applyNpmUpdates": "ncu -u --reject \"webpack,@emotion/core,@emotion/css,emotion,copy-webpack-plugin,webpack-cli,html-webpack-plugin,ts-loader,style-loader,ts-node\"",
    "lint:fix": "eslint --fix \"src/**/*.{ts,tsx}\"",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "pack": "NODE_ENV=production webpack",
    "prettier:apply": "prettier --write \"{,!(dist)}/**/*.{json,yml,yaml,ts,tsx}\"",
    "prettier:check": "prettier --check \"{,!(dist)}/**/*.{json,yml,yaml,ts,tsx}\"",
    "start:webpack5": "NODE_ENV=DEV webpack serve --progress --inline",
    "start": "NODE_ENV=DEV webpack-dev-server --progress --inline",
    "test:report": "sensible-browser ./coverage/lcov-report/index.html",
    "test:browser": "nyc --reporter=lcov polendina src/**/*.spec.ts --webpack-config=./webpack.test.js",
    "test:node": "env TS_NODE_FILES=true TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' nyc --reporter=lcov mocha"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "mocha": {
    "spec": "src/**/*.spec.ts",
    "reporter": "spec",
    "recursive": true,
    "require": [
      "ts-node/register"
    ],
    "exit": true,
    "esmLoader": true
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "check-coverage": true,
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.spec.ts"
    ]
  }
